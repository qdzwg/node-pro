extends layout

block stylesheets
    link(href="#{baseUrl}/stylesheets/swiper-3.4.1.min.css", rel="stylesheet")    
    link(href="#{baseUrl}/stylesheets/newtheme.css?#{vCode}", rel="stylesheet")
block javascripts
    script(src='#{baseUrl}/javascripts/swiper-3.4.1.jquery.min.js', type='text/javascript')
    script(src='#{baseUrl}/javascripts/jquery.SuperSlide.2.1.3.js', type='text/javascript')
    script(src='#{baseUrl}/javascripts/touch-0.2.14.min.js', type='text/javascript')
    script(src='https://webapi.amap.com/maps?v=1.4.10&key=f842f480ae649d8cecb6bf0ede5f21a4', type='text/javascript')
    script(src='#{baseUrl}/javascripts/iconfont.js', type='text/javascript')
    script(src='#{baseUrl}/javascripts/allcomponents.js?#{vCode}', type='text/javascript')
block content
    include ./allcomponents/controller
    include ./allcomponents/common    
    .index-box(style="background-color:"+(pageBackground?pageBackground:'#fff'))
        if newtemplateData.length>0
            each item in newtemplateData
                .index-box-li(class=(item.hideBottom==false||!item.hideBottom)?'boxli-mb05':'')
                    +choiceComponent(item.category, item.data, merchantInfoId)
    if ifshowFoot&&footerData.list&&footerData.list.length>0
        +newfooter(footerData,module)  
           
    .mask#mask
    .tips
        p
        a(href="javascript:;").queding 确定
