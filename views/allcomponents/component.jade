//- 产品分组
mixin productGroup1(datas)
    .productGroup1(class="productGroup1-"+datas.menuType class="pageInter-group1-"+datas.pageInteraction)
        - var grouptabs =  parseInt(datas.list.length)
        - var pergrouptabsWidth = (1/grouptabs)*100+'%'
        ul.productGroup1-ul.clearfix(class="group1-"+datas.menuType+"-ul")
            each item in datas.list
                li(style='width:'+pergrouptabsWidth)
                    a(href='javascript:;') #{item.groupName}
                    if datas.menuType == 'type2'
                        i(class='icon-arrow-down')
        if datas.listType == 'type1' || datas.listType == 'type2'|| datas.listType == 'type3'
            #swiper_pro.swiper-pro(class='productGroup1-list-'+datas.listType)
                if datas.list&&datas.list.length>0
                    each item in datas.list
                        ul.clearfix
                            each it in item.data
                                li
                                    if it.businessType == 'room'
                                        - it.businessType = 'hotel'
                                    if it.businessType == 'mdse'
                                        - it.businessType = 'shop'
                                    if it.businessType == 'route' || item.businessType == 'theater'
                                        - it.priceShow = it.parkPriceShow
                                    a(href='/detail/'+it.businessType+'/'+it.merchantProdcutId+'/'+it.productCode+'?m_id='+merchantInfoId)
                                        img(src=(it.linkMobileImg?it.linkMobileImg:'/images/common/noPic-img.jpg')+'?imageMogr2/thumbnail/640x/strip/quality/100', alt='')
                                        h4 #{it.nickName || it.name}
                                        if it.priceShow
                                            p.price-p
                                                i.icon-rmb ¥
                                                strong.price.c-f63 #{it.priceShow}



