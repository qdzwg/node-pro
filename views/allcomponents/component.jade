//- 店鋪信息
mixin shopInfo(datas)
    .shop-info(style=(datas.backUrl ? 'background:url('+datas.backUrl+') no-repeat center;background-size:100% 100%;': ''))
        .shop-info-content.clearfix
            .shop-info-content-imgs.fl
                img(src="#{datas.logoUrl}?imageMogr2/thumbnail/640x/strip/quality/100")
            .shop-info-content-detail.fl
                p.shop-info-content-detail-title
                    span(style='color:#{datas.textColor}')=datas.name
                    if datas.tel&&datas.tel != ''
                        a(href="tel:"+datas.tel)
                            i.xx-icon.icon-dianhua
                p.shop-info-content-detail-subtitle(style='color:#{datas.textColor}')=datas.introduction

mixin weather(datas)
    .weather-warpper(style='background: '+ datas.backgroundColor,data-item=datas)
        .weather-content
            .weather-content-left
                //- .xx-icon.icon-303(style="font-size:1.3333rem")
                div.weather-icon
                    svg.icon.svg-icon(aria-hidden='true', style='font-size: 1.3333rem;')
                        use(xlink:href='#icon-duoyun')
                div( style='margin-left: 0.25rem;')
                    .weather-left-top
                        span.weather-name=datas.weather
                        span.week 周二
                    .weather-left-bottom
                        span=datas.province
                        span=datas.city
                        span=datas.area
            .weather-content-right #{datas.temperature}℃





