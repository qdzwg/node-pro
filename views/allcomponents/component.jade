include ./common
//导航封装
-
    function gotoTarget(item){
     var _url,_navicon,_nourlclass,_mold;
     switch (item.linkUrl) {
         case 'personal':
             _url = '/member?m_id=' + merchantInfoId
             _navicon = 'icon-member'
             _mold = 'member'
             break;
         case 'home':
             _url = '/?m_id=' + merchantInfoId
             _navicon = 'icon-nav-home'
             _mold = 'index'
             break
         case 'shopcart':
             _url = '/cart?m_id=' + merchantInfoId
             _navicon = 'icon-shopping-car'
             _mold = 'cart'
             break
         case 'ai_p':
             _url = 'javascript:;'
             _navicon = 'icon-AIpaiyipai'
             _nourlclass = 'aipyp'
             break
         case 'order':
             _url = '/list/order?m_id=' + merchantInfoId
             _navicon = 'icon-nav-orders'
             _mold = 'order'
             break
         case 'groupbooking':
             _url = '/group/orderList?m_id=' + merchantInfoId
             _mold = 'group'
             break
         case 'coupon':
             _url = '/coupons/member?m_id=' + merchantInfoId
             _mold = 'coupon'
             break
         case 'receivecoupons':
             _url = '/coupons/list?m_id=' + merchantInfoId
             _mold = 'receivecoupons'
             break
         case 'accountcenter':
             _url = '/member/user?m_id=' + merchantInfoId
             _mold = 'accountcenter'
             break
         case 'ticket':
             _url = '/vue/list/ticket?m_id=' + merchantInfoId
             _navicon = 'icon-nav-ticket'
             _mold = 'ticket'
             break
         case 'hotel':
             _url = '/list/hotel?m_id=' + merchantInfoId
             _navicon = 'icon-nav-hotel'
             _mold = 'hotel'
             break
         case 'shop':
             _url = '/list/shop?m_id=' + merchantInfoId
             _navicon = 'icon-nav-goods'
             _mold = 'shop'
             break
         case 'repast':
             _url = '/list/repast?m_id=' + merchantInfoId
             _navicon = 'icon-nav-foods'
             _mold = 'repast'
             break
         case 'route':
             _url = '/list/route?m_id=' + merchantInfoId
             _navicon = 'icon-nav-tours'
             _mold = 'route'
             break
         case 'strategy':
             _url = '/vue/list/strategy?m_id=' + merchantInfoId
             _navicon = 'icon-nav-raiders'
             _mold = 'strategy'
             break
         case 'pmsHotel':
             _url = '/vue/list/pmsHotel?m_id=' + merchantInfoId
             _navicon = ''
             _mold = 'pmsHotel'
             break
         case 'publicservice':
             _url = 'javascript:;'
             _navicon = ''
             _nourlclass = 'gotoPublic'
             _mold = 'publicservice'
             break
         case 'parkcard':
             _url = 'javascript:;'
             _nourlclass = 'gotoyearcard'
             _navicon = ''
             _mold = 'yearcard'
             break
         case 'custompage':
             _url = '/custompage?id=' + item.customPageId + '&m_id=' + merchantInfoId
             _mold = 'custompage'
             break
         case 'customlink':
             _url = item.customLinkurl
             break
         case 'theater':
             _url = 'javascript:;'
             _nourlclass = 'gototheater'
             _navicon = ''
             break;
         case 'customtel':
             _url = 'tel:'+item.customTel
             _navicon = 'icon-bohao'
             break;
     }
     return {
         _url:_url,
         _nourlclass:_nourlclass,
         _navicon:_navicon,
         _mold:_mold
     }
    }
