//- 产品
mixin product1(datas)
    .product(class='product-'+datas.listType)
        ul
            each item in datas.list
                li
                    if item.businessType == 'room'
                        - item.businessType = 'hotel'
                    if item.businessType == 'mdse'
                        - item.businessType = 'shop'
                    if item.businessType == 'route' || item.businessType == 'theater'
                        - item.priceShow = item.parkPriceShow
                    a(href='/detail/'+item.businessType+'/'+item.merchantProdcutId+'/'+item.productCode+'?m_id='+merchantInfoId)
                        if datas.productTag == 'type4'
                            em.proTag(class=datas.productTag, style='background:url(#{datas.tagUrl}?imageMogr2/thumbnail/640x/strip/quality/100) no-repeat; background-size:100%;')
                        else
                            em.proTag(class=datas.productTag)
                        img.pro-img(src=item.linkMobileImg?item.linkMobileImg+'?imageMogr2/thumbnail/640x/strip/quality/100':'/images/newtheme/pro-type1-img.png')
                        .pro-title
                            p.pro-name #{item.nickName}
                            if item.priceShow
                                p
                                    i.icon-rmb ¥
                                    strong.price.c-f63 #{item.priceShow}


