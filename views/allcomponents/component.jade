mixin newfooter(footerData, module)
    .footBlock
    .newfooter
        ul.clearfix(style='background-color:'+footerData.backgroundColor+'; border-top:1px solid '+footerData.backgroundLineColor)
            - var footNmu = parseInt(footerData.list.length);
            - var footNmuPerW = (1/footNmu)*100+'%';
            - var textColor = footerData.textColor, selectTextColor = footerData.selectTextColor;
            - var iconArr = {'personal':'icon-member','home':'icon-nav-home','order':'icon-nav-orders','shopcart':'icon-shopping-car','ticket': 'icon-nav-ticket','hotel': 'icon-nav-ticket','shop': 'icon-nav-goods','strategy': 'icon-nav-raiders','route': 'icon-nav-tours','repast': 'icon-nav-foods','ai_p': 'icon-AIpaiyipai','customtel':'icon-bohao'};

            each item in footerData.list
                li(style='width:'+footNmuPerW)
                    - var gotoObj = gotoTarget(item)
                    - var _url = gotoObj._url
                    - var _mold = gotoObj._mold
                    - var _nourlclass = gotoObj._nourlclass
                    a(href=_url, class=_nourlclass, style='color:'+(module==_mold?selectTextColor:textColor))

                        if module!=_mold && item.defaultImgUrl!='' 
                            img(src=item.defaultImgUrl)
                        else if module==_mold && item.selectImgUrl !=''
                            img(src=item.selectImgUrl)
                        else
                            i(class='xx-icon '+iconArr[item.linkUrl])
                        span=item.title
script.
    $(function () {
        $("body").on("click",".hot-area-item",function (e) {
            e.stopPropagation()
            var href = $(this).data("href")
            window.location.href=href
        })
    })
